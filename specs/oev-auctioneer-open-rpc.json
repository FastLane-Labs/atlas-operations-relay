  "components": {
    "schemas": {
      "Uint24": {
        "type": "string",
        "description": "Solidity type uint24",
        "pattern": "^0x[0-9a-f]{1,6}$"
      },

      "Uint32": {
        "type": "string",
        "description": "Solidity type uint32",
        "pattern": "^0x[0-9a-f]{1,8}$"
      },

      "Uint256": {
        "type": "string",
        "description": "Solidity type uint256",
        "pattern": "^0x[0-9a-f]{1,64}$"
      },

      "Address": {
        "type": "string",
        "description": "Solidity type address",
        "pattern": "^0x[0-9a-f]{40}$"
      },

      "Bytes": {
        "type": "string",
        "description": "Solidity type bytes",
        "pattern": "^0x([0-9a-f][0-9a-f])*$"
      },

      "Bytes32": {
        "type": "string",
        "description": "Solidity type bytes32",
        "pattern": "^0x[0-9a-f]{64}$"
      },

      "AuctionParameters": {
        "type": "object",
        "properties": {
          "chainId": {
            "$ref": "#/components/schemas/Uint256"
          },
          "adapter": {
            "$ref": "#/components/schemas/Address"
          },
          "updatePayload": {
            "$ref": "#/components/schemas/Bytes"
          },
          "earlyReturn": {
            "type": "boolean"
          },
          "bundlerEoa": {
            "$ref": "#/components/schemas/Address"
          },
          "maxFeePerGas": {
            "$ref": "#/components/schemas/Uint256"
          },
          "signature": {
            "$ref": "#/components/schemas/Bytes"
          }
        },
        "required": [
          "chainId",
          "adapter",
          "updatePayload",
          "bundlerEoa",
          "signature"
        ]
      },

      "UserOperation": {
        "type": "object",
        "description": "Atlas user operation with all fields hex encoded",
        "properties": {
          "from": {
            "$ref": "#/components/schemas/Address"
          },
          "to": {
            "$ref": "#/components/schemas/Address"
          },
          "value": {
            "$ref": "#/components/schemas/Uint256"
          },
          "gas": {
            "$ref": "#/components/schemas/Uint256"
          },
          "maxFeePerGas": {
            "$ref": "#/components/schemas/Uint256"
          },
          "nonce": {
            "$ref": "#/components/schemas/Uint256"
          },
          "deadline": {
            "$ref": "#/components/schemas/Uint256"
          },
          "dapp": {
            "$ref": "#/components/schemas/Address"
          },
          "control": {
            "$ref": "#/components/schemas/Address"
          },
          "callConfig": {
            "$ref": "#/components/schemas/Uint32"
          },
          "dappGasLimit": {
            "$ref": "#/components/schemas/Uint32"
          },
          "solverGasLimit": {
            "$ref": "#/components/schemas/Uint32"
          },
          "bundlerSurchargeRate": {
            "$ref": "#/components/schemas/Uint24"
          },
          "sessionKey": {
            "$ref": "#/components/schemas/Address"
          },
          "data": {
            "$ref": "#/components/schemas/Bytes"
          },
          "signature": {
            "$ref": "#/components/schemas/Bytes"
          }
        },
        "required": [
          "from",
          "to",
          "value",
          "gas",
          "maxFeePerGas",
          "nonce",
          "deadline",
          "dapp",
          "control",
          "callConfig",
          "dappGasLimit",
          "solverGasLimit",
          "bundlerSurchargeRate",
          "sessionKey",
          "data",
          "signature"
        ]
      },

      "SolverOperation": {
        "type": "object",
        "description": "Atlas solver operation with all fields hex encoded",
        "properties": {
          "from": {
            "$ref": "#/components/schemas/Address"
          },
          "to": {
            "$ref": "#/components/schemas/Address"
          },
          "value": {
            "$ref": "#/components/schemas/Uint256"
          },
          "gas": {
            "$ref": "#/components/schemas/Uint256"
          },
          "maxFeePerGas": {
            "$ref": "#/components/schemas/Uint256"
          },
          "deadline": {
            "$ref": "#/components/schemas/Uint256"
          },
          "solver": {
            "$ref": "#/components/schemas/Address"
          },
          "control": {
            "$ref": "#/components/schemas/Address"
          },
          "userOpHash": {
            "$ref": "#/components/schemas/Bytes32"
          },
          "bidToken": {
            "$ref": "#/components/schemas/Address"
          },
          "bidAmount": {
            "$ref": "#/components/schemas/Uint256"
          },
          "data": {
            "$ref": "#/components/schemas/Bytes"
          },
          "signature": {
            "$ref": "#/components/schemas/Bytes"
          }
        },
        "required": [
          "from",
          "to",
          "value",
          "gas",
          "maxFeePerGas",
          "deadline",
          "solver",
          "control",
          "userOpHash",
          "bidToken",
          "bidAmount",
          "data",
          "signature"
        ]
      },

      "DAppOperation": {
        "type": "object",
        "description": "Atlas dApp operation with all fields hex encoded",
        "properties": {
          "from": {
            "$ref": "#/components/schemas/Address"
          },
          "to": {
            "$ref": "#/components/schemas/Address"
          },
          "nonce": {
            "$ref": "#/components/schemas/Uint256"
          },
          "deadline": {
            "$ref": "#/components/schemas/Uint256"
          },
          "control": {
            "$ref": "#/components/schemas/Address"
          },
          "bundler": {
            "$ref": "#/components/schemas/Address"
          },
          "userOpHash": {
            "$ref": "#/components/schemas/Bytes32"
          },
          "callChainHash": {
            "$ref": "#/components/schemas/Bytes32"
          },
          "signature": {
            "$ref": "#/components/schemas/Bytes"
          }
        },
        "required": [
          "from",
          "to",
          "nonce",
          "deadline",
          "control",
          "bundler",
          "userOpHash",
          "callChainHash",
          "signature"
        ]
      }
    }
  }
}
